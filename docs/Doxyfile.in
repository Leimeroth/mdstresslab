PROJECT_NAME           = "MDStressLab++"
MARKDOWN_SUPPORT       = YES
BUILTIN_STL_SUPPORT    = YES
EXTRACT_ALL            = YES
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/doc_doxygen/
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/src/ @CMAKE_CURRENT_SOURCE_DIR@/include/ @CMAKE_CURRENT_SOURCE_DIR@/tests/ 
EXAMPLE_PATH           = @CMAKE_CURRENT_SOURCE_DIR@/unit_tests/  @CMAKE_CURRENT_SOURCE_DIR@/tests/
EXCLUDE                = @CMAKE_CURRENT_SOURCE_DIR@/include/MDStressLab++/Eigen/
USE_MATHJAX            = YES
MATHJAX_VERSION        = MathJax_2
MATHJAX_FORMAT         = HTML-CSS
MATHJAX_RELPATH        = https://cdn.jsdelivr.net/npm/mathjax@2
SEARCH_INCLUDES        = YES
EXTRACT_STATIC         = YES
RECURSIVE              = YES
SOURCE_BROWSER         = YES
HTML_COLORSTYLE        = AUTO_LIGHT
GENERATE_TREEVIEW      = YES
